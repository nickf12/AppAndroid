<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.listasuperapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/shopping_basket_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainSandra" />
        <activity android:name=".MainNatalia" />
        <activity android:name=".ListaActivity" />
        <activity android:name=".ListaView" />
        <activity android:name=".OfertasActivity" />
        <activity android:name=".ListSelector" />
        <activity android:name=".Productos"></activity>
    </application>

</manifest>